{
  "name": "interface",
  "productName": "interface",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsc && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "dev": "concurrently \"tsc -w\" \"electron-forge start\"",
    "package-mac": "electron-packager . \"Tempmail GUI\" --overwrite --platform=darwin --arch=x64 --icon=icons/mac/icon.icns --prune=true --out=release-builds",
    "package-linux": "electron-packager . \"Tempmail GUI\" --overwrite --asar --platform=linux --arch=x64 --icon=icons/png/1024x1024.png --prune=true --out=release-builds",
    "package-win": "electron-packager . \"Tempmail GUI\" --overwrite --asar --platform=win32 --arch=ia32 --icon=icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Tempmail GUI\""
  },
  "keywords": [],
  "author": {
    "name": "cookie-for-pres",
    "email": "cookiedid911@protonmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Tempmail GUI",
            "description": "A Tempmail GUI Made By: cookie",
            "author": "cookie",
            "overwrite": true
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux",
            "win32"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "name": "Tempmail GUI",
            "description": "A Tempmail GUI Made By: cookie",
            "author": "cookie",
            "overwrite": true
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {
            "name": "Tempmail GUI",
            "description": "A Tempmail GUI Made By: cookie",
            "author": "cookie",
            "overwrite": true
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "17.1.2",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
